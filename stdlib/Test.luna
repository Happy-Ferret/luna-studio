% + OrphanNames

foreign haskell def print s:
    polyJoin . liftF1 (Value . fmap Safe . print) $ s

foreign haskell def Int.+ a:
    liftF2 (+) self a

def a + b: a.+ b

class Vector a:
    Vector : x,y,z :: a
    Scalar : x     :: a

def main:
    v = Vector 1 2 3
    v.x = 10
    
    foo = x : x + 1
    print v