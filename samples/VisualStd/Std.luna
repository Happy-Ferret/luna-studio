class Vector a:
    x,y,z :: a

    def + self v:
        Vector (x + v.x) (y + v.y) (z + v.z)

    def length self:
        (x*x + y*y + z*z).sqrt

    def normalize self:
        Vector (x/length) (y/length) (z/length)